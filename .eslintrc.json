{
  "env": {
    "node": true,
    "commonjs": true,
    "es2021": true,
    "browser": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:@typeScript-eslint/recommended",
    "prettier"
  ],
  "parser": "@typeScript-eslint/parser",
  "parserOptions": {
    //   "ecmaVersion": 12
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typeScript-eslint"],
  "rules": {
    //   "no-console": "off",
    //   "indent": ["error", 2],
    //   "quotes": ["error", "single"],
    //   "semi": ["error", "always"]
    // "no-used-vars":"error",
    "no-unused-vars": "error",
    "no-unused-expressions": "error",
    "prefer-const": "error",
    // "no-console": "warning",
    "no-console": "warn",
    "no-undef": "error"
  },
  "globals": {
    "process": "readonly"
  }
}
